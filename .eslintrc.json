{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "overrides": [
      {
        "files": "src/**/*.ts"
      }
    ],
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "no-console": "off",
      "no-restricted-syntax": "off",
      "no-underscore-dangle": "off",
      "no-plusplus": "off",
      "consistent-return": "off",
      "max-classes-per-file": "off",
      "import/no-unresolved": "off",
      "import/extensions": "off"
    }
}
